<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Vest\ResponsaBundle\Entity\VaVeranstaltungen" table="va_veranstaltungen">
    <id name="veranstaltungId" type="integer" column="veranstaltung_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="veranstaltungTag" type="string" column="veranstaltung_tag" length="255" nullable="false"/>
    <field name="veranstaltungName" type="string" column="veranstaltung_name" length="255" nullable="true"/>
    <field name="veranstaltungBeschreibung" type="text" column="veranstaltung_beschreibung" nullable="true"/>
    <field name="kontaktId" type="integer" column="kontakt_id" nullable="false"/>
    <field name="kategorieId" type="integer" column="kategorie_id" nullable="false"/>
    <field name="veranstaltungPdfOrder" type="integer" column="veranstaltung_pdf_order" nullable="false"/>
    <field name="veranstaltungtIstpres" type="integer" column="veranstaltungt_istpres" nullable="false"/>
    <field name="veranstaltungTipp" type="string" column="veranstaltung_tipp" length="10" nullable="false"/>
    <field name="veranstaltungNeu" type="string" column="veranstaltung_neu" length="10" nullable="false"/>
  </entity>
</doctrine-mapping>
