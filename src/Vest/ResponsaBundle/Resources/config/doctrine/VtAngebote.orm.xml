<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Vest\ResponsaBundle\Entity\VtAngebote" table="vt_angebote">
    <id name="angebotId" type="bigint" column="angebot_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="kontaktId" type="bigint" column="kontakt_id" nullable="false"/>
    <field name="angebotName" type="string" column="angebot_name" length="255" nullable="false"/>
    <field name="angebotBeschreibung" type="text" column="angebot_beschreibung" nullable="false"/>
    <field name="angebotNutzung" type="integer" column="angebot_nutzung" nullable="false"/>
    <field name="angebotDauer" type="string" column="angebot_dauer" length="25" nullable="false"/>
    <field name="angebotCat" type="integer" column="angebot_cat" nullable="false"/>
    <field name="angebotReqPerson" type="boolean" column="angebot_req_person" nullable="false"/>
    <field name="angebotIntern" type="boolean" column="angebot_intern" nullable="false"/>
  </entity>
</doctrine-mapping>
