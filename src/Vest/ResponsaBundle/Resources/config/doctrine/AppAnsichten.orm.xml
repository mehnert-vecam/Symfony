<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Vest\ResponsaBundle\Entity\AppAnsichten" table="app_ansichten">
    <id name="ansichtId" type="integer" column="ansicht_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="anwendungId" type="integer" column="anwendung_id" nullable="false"/>
    <field name="ansichtName" type="string" column="ansicht_name" length="255" nullable="false"/>
    <field name="ansichtKurzname" type="string" column="ansicht_kurzname" length="255" nullable="false"/>
    <field name="ansichtStatus" type="string" column="ansicht_status" length="10" nullable="false"/>
    <field name="ansichtPrio" type="integer" column="ansicht_prio" nullable="false"/>
  </entity>
</doctrine-mapping>
