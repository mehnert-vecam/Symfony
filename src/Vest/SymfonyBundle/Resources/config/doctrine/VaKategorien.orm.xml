<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Vest\SymfonyBundle\Entity\VaKategorien" table="va_kategorien">
    <id name="kategorieId" type="integer" column="kategorie_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="kategorieName" type="string" column="kategorie_name" length="255" nullable="false"/>
    <field name="kategorieAnmeldebedingung" type="text" column="kategorie_anmeldebedingung" nullable="false"/>
    <field name="kontaktId" type="bigint" column="kontakt_id" nullable="false"/>
    <field name="kategorieReq" type="integer" column="kategorie_req" nullable="false"/>
    <field name="kategorieSplit" type="integer" column="kategorie_split" nullable="false"/>
    <field name="kategoriePrio" type="integer" column="kategorie_prio" nullable="false"/>
  </entity>
</doctrine-mapping>
