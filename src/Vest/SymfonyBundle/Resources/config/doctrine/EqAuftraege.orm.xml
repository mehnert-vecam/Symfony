<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Vest\SymfonyBundle\Entity\EqAuftraege" table="eq_auftraege">
    <id name="auftragId" type="bigint" column="auftrag_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="auftragStartdatum" type="datetime" column="auftrag_startdatum" nullable="true"/>
    <field name="auftragEnddatum" type="date" column="auftrag_enddatum" nullable="true"/>
    <field name="auftragStatus" type="string" column="auftrag_status" length="200" nullable="true"/>
    <field name="anlassId" type="bigint" column="anlass_id" nullable="false"/>
    <field name="personId" type="bigint" column="person_id" nullable="false"/>
    <field name="datumId" type="bigint" column="datum_id" nullable="false"/>
  </entity>
</doctrine-mapping>
