<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Vest\SymfonyBundle\Entity\BnrBanner" table="bnr_banner">
    <id name="bannerId" type="integer" column="banner_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bannerName" type="string" column="banner_name" length="45" nullable="true"/>
    <field name="bannerDatei" type="string" column="banner_datei" length="45" nullable="true"/>
    <field name="bannerFormat" type="string" column="banner_format" length="45" nullable="true"/>
    <field name="bannerPrio" type="bigint" column="banner_prio" nullable="true"/>
    <field name="kontaktId" type="bigint" column="kontakt_id" nullable="true"/>
  </entity>
</doctrine-mapping>
