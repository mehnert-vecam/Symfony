<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Vest\SymfonyBundle\Entity\AppAnwendungen" table="app_anwendungen">
    <id name="anwendungId" type="integer" column="anwendung_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="anwendungName" type="string" column="anwendung_name" length="255" nullable="false"/>
    <field name="anwendungBeschreibung" type="text" column="anwendung_beschreibung" nullable="false"/>
    <field name="anwendungKurzname" type="string" column="anwendung_kurzname" length="255" nullable="false"/>
    <field name="anwendungUberId" type="integer" column="anwendung_uber_id" nullable="false"/>
    <field name="anwendungAnzeigePrio" type="integer" column="anwendung_anzeige_prio" nullable="false"/>
    <field name="anwendungFarbcode" type="string" column="anwendung_farbcode" length="255" nullable="false"/>
    <field name="anwendungIcon" type="string" column="anwendung_icon" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
