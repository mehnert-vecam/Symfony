<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Vest\SymfonyBundle\Entity\NlTemplates" table="nl_templates">
    <id name="templateId" type="integer" column="template_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="templateName" type="string" column="template_name" length="45" nullable="true"/>
    <field name="templateObjekt" type="text" column="template_objekt" nullable="true"/>
    <field name="kontaktId" type="bigint" column="kontakt_id" nullable="true"/>
  </entity>
</doctrine-mapping>
