<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Vest\SymfonyBundle\Entity\AddrKontakte" table="addr_kontakte">
    <indexes>
      <index name="record_status" columns="record_status"/>
    </indexes>
    <id name="kontaktId" type="bigint" column="kontakt_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="kontaktKdnr" type="string" column="kontakt_kdnr" length="255" nullable="false"/>
    <field name="kontaktTyp" type="string" column="kontakt_typ" length="50" nullable="false"/>
    <field name="kontaktAnrede" type="string" column="kontakt_anrede" length="50" nullable="false"/>
    <field name="kontaktName" type="string" column="kontakt_name" length="100" nullable="false"/>
    <field name="kontaktName2" type="string" column="kontakt_name2" length="75" nullable="false"/>
    <field name="kontaktName3" type="string" column="kontakt_name3" length="75" nullable="false"/>
    <field name="kontaktKurzname" type="string" column="kontakt_kurzname" length="255" nullable="false"/>
    <field name="kontaktStrasse" type="string" column="kontakt_strasse" length="255" nullable="false"/>
    <field name="kontaktPlz" type="string" column="kontakt_plz" length="10" nullable="false"/>
    <field name="kontaktOrt" type="string" column="kontakt_ort" length="255" nullable="false"/>
    <field name="kontaktTelefon" type="string" column="kontakt_telefon" length="100" nullable="false"/>
    <field name="kontaktTelefax" type="string" column="kontakt_telefax" length="100" nullable="false"/>
    <field name="kontaktEmail" type="string" column="kontakt_email" length="255" nullable="false"/>
    <field name="kontaktUrl" type="string" column="kontakt_url" length="255" nullable="false"/>
    <field name="kontaktBemerkung" type="text" column="kontakt_bemerkung" nullable="false"/>
    <field name="kontaktAktualisiert" type="datetime" column="kontakt_aktualisiert" nullable="false"/>
    <field name="statusId" type="integer" column="status_id" nullable="false"/>
    <field name="parentId" type="bigint" column="parent_id" nullable="false"/>
    <field name="protectionLevel" type="integer" column="protection_level" nullable="false"/>
    <field name="recordStatus" type="boolean" column="record_status" nullable="false"/>
  </entity>
</doctrine-mapping>
